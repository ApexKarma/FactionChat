        </dependency>
        <dependency>
            <groupId>com.massivecraft</groupId>
            <artifactId>factions</artifactId>
            <version>1.8.2</version>
            <scope>system</scope>
            <systemPath>${basedir}/lib/Factions 1.8.2 UUID.jar</systemPath>
